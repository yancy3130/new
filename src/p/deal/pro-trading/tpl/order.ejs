<li class="item clearfix">
    <div class="left wrapper">
        <h4>开仓价格：</h4>
        <input id="J_OpenPriceInput" class="J_Edit" type="text" value="<%= data.openprice || (data.openPrice || data.price) %>" <% if (!data.guadan) {%> disabled <% } %>>
        <p>占用资金（$）<%= data.margin === undefined ? '' : parseFloat(parseFloat(data.margin).toFixed(2)) %></p>
    </div>
    <div class="wrapper">
        <h4>交易量（手）：</h4>
        <input type="text" class="J_Edit" id="J_Volumn" value="<%= data.volume %>" <% if (data.edit) { %> disabled <%}%>>
        <p>最大可买（手）<span id="J_MaxVolumn"></span></p>
    </div>
    <span class="bar"></span>
</li>
<li class="item clearfix">
    <div class="left wrapper">
        <h4>止盈价格：</h4>
        <input type="text" id="J_Profit" value="<%= parseFloat(data.takeProfit) === 0 ? '' : data.takeProfit   %>">
        <p>目标盈利（$）<span id="J_ProfitCount"></span></p>
    </div>
    <div class="wrapper">
        <h4>止损价格：</h4>
        <input type="text" id="J_Loss" value="<%= parseFloat(data.stopLoss) === 0 ? '' : data.stopLoss %>">
        <p>预期亏损（$）<span id="J_LossCount"></span></p>
    </div>
    <span class="bar"></span>
</li>

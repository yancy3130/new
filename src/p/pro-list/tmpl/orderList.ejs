<% data.forEach(function(item) { %>
    <%if (item.status == 'open') { %>
        <% var isUp = item.cmd.indexOf('buy') !== -1 %>
        <li class="item ui common circle-list J_Item <%= parseFloat(item.profit) > 0 ? 'up' : 'down' %>" data-symbol="<%= item.symbol %>" data-ticket="<%= item.ticket %>">
            <p class="J_Profit"><%= item.profit %></p>
            <p class="cmd"><%= isUp ? '买涨' : '买跌' %></p>
        </li>
    <% } %>
<% }) %>
<%if(data.length == 0) {%>
    <li class="no-order-item">
        <span class="no-order-text-item">暂无相关订单</span>
    </li>
<%}%>
<div class="img-wrapper-outer">
  <div class="img-wrapper">
    <img class="img" src="<%= data.avatar %>" alt="">
  </div>
</div>
<h2 class="name"><%= data.nick_name %></h2>
<div class="detail-wrapper">
  <div class="left">
    <p>
      <span class="small">$</span>
      <span class="J_TotalProfit"><%= parseInt(data.gross_profit||0) %></span>
    </p>
    <p class="desc">全时收益</p>
  </div>
  <div class="right">
    <p><%= parseInt(data.month_rate_of_return * 100 || 0) %> <span class="small">%</span></p>
    <p class="desc">月均收益率</p>
  </div>
  <a href="./data.html?inviteCode=<%= data.inviteCode %>&from=<%= encodeURIComponent(location.href) %>" class="link">数据分析</a>
</div>